#include<iostream>
using namespace std;

int main() {
    string x;
    string y;
    cout << "enter x"<<endl;
    cin>>x;
    cout << "enter y"<<endl;
    cin>>y;
   string str_inp1(x);
   string str_inp2(y);

cout<<"String 1:"<<str_inp1<<endl;
cout<<"String 2:"<<str_inp2<<endl;
int res = str_inp1.compare(str_inp2);
     cout<<endl;
    if (res == 0)
        cout << "Both the input strings are same" << endl;
    else 
        cout << "Strings are not same"<<endl;
}
